{
  "name": "@peter554/cssutils",
  "version": "0.9.1",
  "description": "A CSS utility class generator. User friendly, simple, powerful.",
  "keywords": [
    "css",
    "functional css",
    "utility-first css",
    "utility-classes",
    "design-tokens"
  ],
  "license": "MIT",
  "repository": "peter554/cssutils",
  "scripts": {
    "test": "jest",
    "prettier:check": "prettier --check src",
    "build:docs": "npm run example:css && node dev/makeReadme.js",
    "example:start": "http-server example",
    "example:css": "node src/cli.js -nm -c example/cssutils.yml -o example/utils.css"
  },
  "files": [
    "src/"
  ],
  "main": "./src/generate.js",
  "bin": {
    "cssutils": "./src/cli.js"
  },
  "dependencies": {
    "clean-css": "^4.2.3",
    "commander": "^5.1.0",
    "cosmiconfig": "^6.0.0",
    "shelljs": "^0.8.4",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "http-server": "^0.12.3",
    "jest": "^26.1.0",
    "markdown-toc": "^1.2.0",
    "prettier": "^2.0.5"
  }
}
